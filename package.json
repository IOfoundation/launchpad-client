{
  "name": "launchpad-client",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "npm": "5.6.0",
    "node": "8.2.1"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run test",
    "build": "babel-node tools/build.js",
    "test": "jest",
    "test:CI": "./node_modules/.bin/babel-node tools/testCi.js",
    "test:cover": "npm run test -- --coverage ",
    "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch",
    "heroku-postbuild": "npm run build",
    "start:prod": "node ./deploy/index.js",
    "open:cover": "npm run test:cover && opn ./coverage/lcov-report/index.html",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js",
    "eslint": "node_modules/.bin/eslint --fix --ext \".js\" --ignore-pattern node_modules --quiet .",
    "prettier": "node_modules/.bin/prettier --single-quote --bracket-spacing false --trailing-comma es5 --write '{src,__{tests,mocks}__}/**/*.js'"
  },
  "author": "FullStack Labs",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "3.9.2",
    "axios": "0.17.0",
    "chai": "4.1.2",
    "deep-freeze": "^0.0.1",
    "express": "4.16.2",
    "flexboxgrid": "6.3.1",
    "google-map-react": "^0.27.0",
    "jsonapi-serializer": "3.5.5",
    "object-assign": "4.1.0",
    "prop-types": "15.5.10",
    "query-string": "^4.3.4",
    "react": "16.8.1",
    "react-dom": "16.8.1",
    "react-onclickoutside": "6.7.1",
    "react-paginate": "6.2.1",
    "react-redux": "6.0.0",
    "react-router": "3.0.5",
    "react-router-redux": "4.0.8",
    "react-tabs": "^3.0.0",
    "react-tap-event-plugin": "3.0.3",
    "react-test-renderer": "16.8.1",
    "redux": "4.0.1",
    "redux-thunk": "^2.3.0",
    "url-parse": "1.4.4",
    "webpack-dotenv-plugin": "^2.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.0",
    "@babel/node": "7.0.0",
    "@babel/plugin-proposal-class-properties": "7.0.0",
    "@babel/plugin-transform-react-constant-elements": "7.0.0",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.15",
    "babel-polyfill": "6.26.0",
    "browser-sync": "2.18.5",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "coveralls": "2.11.15",
    "css-loader": "0.26.1",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.9.1",
    "enzyme-to-json": "3.3.5",
    "eslint": "3.19.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-prettier": "2.3.1",
    "eslint-plugin-react": "6.10.3",
    "eslint-watch": "2.1.14",
    "extract-text-webpack-plugin": "2.1.2",
    "fetch-mock": "5.13.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.24.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.1.0",
    "jest-cli": "24.1.0",
    "jest-fetch-mock": "2.1.0",
    "json-loader": "0.5.4",
    "mockdate": "2.0.1",
    "moxios": "^0.4.0",
    "nock": "9.0.22",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.1.1",
    "opn-cli": "3.1.0",
    "postcss-loader": "1.2.1",
    "prettier": "1.16.4",
    "prompt": "1.0.0",
    "react-addons-test-utils": "15.6.2",
    "react-hot-loader": "3.0.0-beta.6",
    "redux-immutable-state-invariant": "1.2.4",
    "redux-mock-store": "^1.2.3",
    "replace": "0.3.0",
    "rimraf": "2.5.4",
    "sass-loader": "6.0.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "2.2.1",
    "webpack-bundle-analyzer": "2.1.1",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.17.1",
    "webpack-md5-hash": "0.0.5"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": ""
  },
  "jest": {
    "setupFiles": [
      "./src/tests/helpers/testHelpers.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "^.+\\.(gif|ttf|eot|svg|woff|woff2|ico)$": "<rootDir>/tools/fileMock.js"
    },
    "collectCoverage": true,
    "coverageReporters": [
      "html",
      "text-summary"
    ],
    "verbose": true
  }
}
