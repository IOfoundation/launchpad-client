{
  "name": "launchpad-client",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "npm": "6.4.1",
    "node": ">=8.0.0 <11.0.0"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run test",
    "pre-commit": "lint-staged",
    "build": "babel-node tools/build.js",
    "test": "jest",
    "test:CI": "./node_modules/.bin/babel-node tools/testCi.js",
    "test:cover": "npm run test -- --coverage",
    "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch",
    "heroku-postbuild": "npm run build",
    "start:prod": "node ./deploy/index.js",
    "open:cover": "npm run test:cover && opn ./coverage/lcov-report/index.html",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js",
    "eslint": "node_modules/.bin/eslint --fix --ext \".js\" --ignore-pattern node_modules --quiet .",
    "prettier": "node_modules/.bin/prettier --single-quote --bracket-spacing false --trailing-comma es5 --write '{src,__{tests,mocks}__}/**/*.js'"
  },
  "author": "FullStack Labs",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "7.3.1",
    "@material-ui/core": "3.9.2",
    "@progress/kendo-scheduler-react-wrapper": "2018.3.1017",
    "@progress/kendo-theme-default": "3.3.1",
    "@progress/kendo-ui": "2019.1.227",
    "axios": "0.17.0",
    "chai": "4.1.2",
    "deep-freeze": "^0.0.1",
    "dompurify": "1.0.10",
    "express": "4.16.2",
    "flexboxgrid": "6.3.1",
    "formik": "1.5.1",
    "google-map-react": "^0.27.0",
    "jsonapi-serializer": "3.5.5",
    "object-assign": "4.1.0",
    "prop-types": "15.5.10",
    "query-string": "^4.3.4",
    "react": "16.8.1",
    "react-dom": "16.8.1",
    "react-medium-editor": "1.8.1",
    "react-onclickoutside": "6.7.1",
    "react-paginate": "6.2.1",
    "react-redux": "6.0.0",
    "react-router": "3.0.5",
    "react-router-redux": "4.0.8",
    "react-swipeable-views": "0.13.1",
    "react-tabs": "^3.0.0",
    "react-tap-event-plugin": "3.0.3",
    "react-test-renderer": "16.8.1",
    "redux": "4.0.1",
    "redux-thunk": "^2.3.0",
    "url-parse": "1.4.4",
    "webpack-dotenv-plugin": "^2.1.0",
    "yup": "0.26.10"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.2.2",
    "@babel/node": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.3.0",
    "@babel/plugin-transform-react-constant-elements": "7.2.0",
    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/polyfill": "7.2.5",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-stage-0": "7.0.0",
    "@babel/register": "7.0.0",
    "autoprefixer": "9.4.7",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "babel-polyfill": "6.26.0",
    "browser-sync": "2.18.5",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "coveralls": "2.11.15",
    "css-loader": "2.1.0",
    "cssnano": "4.1.8",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.9.1",
    "enzyme-to-json": "3.3.5",
    "eslint": "5.13.0",
    "eslint-config-prettier": "4.0.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-watch": "4.0.2",
    "fetch-mock": "5.13.1",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.1.0",
    "jest-cli": "24.1.0",
    "jest-fetch-mock": "2.1.0",
    "json-loader": "0.5.7",
    "lint-staged": "8.1.3",
    "mini-css-extract-plugin": "0.5.0",
    "mockdate": "2.0.2",
    "moxios": "^0.4.0",
    "nock": "9.0.22",
    "node-sass": "4.11.0",
    "npm-run-all": "^4.1.1",
    "opn-cli": "3.1.0",
    "postcss-loader": "3.0.0",
    "pre-commit": "1.2.2",
    "prettier": "1.16.4",
    "prompt": "1.0.0",
    "react-addons-test-utils": "15.6.2",
    "react-hot-loader": "3.0.0-beta.6",
    "redux-immutable-state-invariant": "1.2.4",
    "redux-mock-store": "1.5.3",
    "replace": "0.3.0",
    "rimraf": "2.5.4",
    "sass-loader": "7.1.0",
    "style-loader": "0.13.1",
    "uglifyjs-webpack-plugin": "2.1.1",
    "url-loader": "1.1.2",
    "webpack": "4.29.3",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-dev-middleware": "3.5.2",
    "webpack-hot-middleware": "2.24.3",
    "webpack-md5-hash": "0.0.6"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": ""
  },
  "jest": {
    "setupFiles": [
      "./src/tests/helpers/testHelpers.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      "^.+\\.(gif|ttf|eot|svg|woff|woff2|ico)$": "<rootDir>/tools/fileMock.js",
      "@Actions(.*)$": "<rootDir>/src/actions/$1",
      "@Shared(.*)$": "<rootDir>/src/components/shared/$1",
      "@Utils(.*)$": "<rootDir>/src/utils/$1"
    },
    "coverageReporters": [
      "html",
      "text-summary"
    ],
    "verbose": true
  },
  "pre-commit": [
    "pre-commit"
  ],
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --bracket-spacing false --trailing-comma es5 --write '{src,__{tests,mocks}__}/**/*.js'",
      "git add"
    ]
  }
}
